<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="edu.conformity.mapper.RoleMapper">

	<select id="getById" resultType="edu.conformity.pojo.Role">
		SELECT
			*
		FROM `t_role`
		WHERE id = ${id}
	</select>

</mapper>